{
    "name": "foundry-framework/framework",
    "description": "The Foundry Framework is an implementation of the Laravel Framework with an opinionated approach using Doctrine and Services to build core API driven applications.",
    "keywords": ["framework", "foundry"],
    "license": "MIT",
    "homepage": "https://www.foundryframework.com",
    "support": {
        "issues": "https://github.com/foundry-framework/framework/issues",
        "source": "https://github.com/foundry-framework/framework"
    },
    "authors": [
        {
            "name": "Greg Gunner",
            "email": "greg@co-foundry.co.za"
        },
        {
            "name": "Medard Ilunga",
            "email": "medard@co-foundry.co.za"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-doctrine/orm": "1.4.*",
        "laravel/framework": "5.6.*",
        "nette/php-generator": "3.1.*",
        "laravel-doctrine/migrations": "1.1.*",
        "gedmo/doctrine-extensions": "^2.4",
        "beberlei/DoctrineExtensions": "^1.0",
        "laravel-doctrine/extensions": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Foundry\\Framework\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foundry\\Framework\\FoundryServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "suggest": {
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
